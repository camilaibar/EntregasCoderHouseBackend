<div>
    <h1>Lista de productos</h1>
    <table>
        <tr>
            {{#each tableHeader}}
                <th>{{this}}</th>
            {{/each}}
        </tr>
        {{#if hasRows}}
            {{#each tableRows}}
                <tr>
                    <td>{{this.pid}}</td>
                    <td>{{this.title}}</td>
                    <td>{{this.description}}</td>
                    <td>{{this.code}}</td>
                    <td>{{this.category}}</td>
                    <td>{{this.price}}</td>
                    <td>{{this.thumbnail}}</td>
                    <td>{{this.stock}}</td>
                    <td>{{this.status}}</td>
                </tr>
            {{/each}}
        {{else}}
            <tr>
                <td>No products loaded</td>
            </tr>
        {{/if}}
    </table>
</div>